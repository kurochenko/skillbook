{
  "name": "skillbook",
  "version": "0.1.5",
  "description": "Manage AI coding assistant skills across projects",
  "type": "module",
  "bin": {
    "skillbook": "./src/cli.ts"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build src/cli.ts --compile --minify --sourcemap --outfile dist/skillbook",
    "build:all": "bun run build:darwin-arm64 && bun run build:darwin-x64 && bun run build:linux-x64 && bun run build:linux-arm64",
    "build:darwin-arm64": "bun build src/cli.ts --compile --minify --sourcemap --target=bun-darwin-arm64 --outfile dist/skillbook-darwin-arm64",
    "build:darwin-x64": "bun build src/cli.ts --compile --minify --sourcemap --target=bun-darwin-x64 --outfile dist/skillbook-darwin-x64",
    "build:linux-x64": "bun build src/cli.ts --compile --minify --sourcemap --target=bun-linux-x64 --outfile dist/skillbook-linux-x64",
    "build:linux-arm64": "bun build src/cli.ts --compile --minify --sourcemap --target=bun-linux-arm64 --outfile dist/skillbook-linux-arm64",
    "test": "bun test"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.9",
    "@types/semver": "^7.5.8",
    "ink-testing-library": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "citty": "^0.2.0",
    "fdir": "^6.5.0",
    "ink": "^6.6.0",
    "picocolors": "^1.1.1",
    "react": "^19.2.3",
    "react-devtools-core": "^7.0.1",
    "semver": "^7.6.3"
  }
}
